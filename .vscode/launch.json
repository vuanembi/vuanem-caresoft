{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "DataFlow",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/dataflow",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "--input",
                "${input:input}"
            ],
        }
    ],
    "inputs": [
        {
            "id": "input",
            "type": "pickString",
            "options": [
                "{\"\"table\"\": \"\"Agents\"\"}",
                "{\"\"table\"\": \"\"Groups\"\"}",
                "{\"\"table\"\": \"\"Services\"\"}",
                "{\"\"table\"\": \"\"TicketsCustomFields\"\"}",
                "{\"\"table\"\": \"\"ContactsCustomFields\"\"}",
                "{\"\"table\"\": \"\"Contacts\"\", \"\"updated_since\"\": \"\"2022-05-02T00:00:00Z\"\", \"\"updated_to\"\": \"\"2022-05-03T00:00:00Z\"\"}",
                "{\"\"table\"\": \"\"Tickets\"\", \"\"updated_since\"\": \"\"2022-05-01T00:00:00Z\"\", \"\"updated_to\"\": \"\"2022-05-03T00:00:00Z\"\"}",
            ],
            "description": "Input"
        }
    ]
}
